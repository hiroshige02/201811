<p id="notice"><%= notice %></p>

<h1>Equipment Events</h1>

<table>
  <thead>
    <tr>
      <th>Event</th>
      <th>Equipment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @equipment_events.each do |equipment_event| %>
      <tr>
        <td><%= equipment_event.event_id %></td>
        <td><%= equipment_event.equipment_id %></td>
        <td><%= link_to 'Show', equipment_event %></td>
        <td><%= link_to 'Edit', edit_equipment_event_path(equipment_event) %></td>
        <td><%= link_to 'Destroy', equipment_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Equipment Event', new_equipment_event_path %>
