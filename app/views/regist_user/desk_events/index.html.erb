<p id="notice"><%= notice %></p>

<h1>Desk Events</h1>

<table>
  <thead>
    <tr>
      <th>Event</th>
      <th>Desk</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @desk_events.each do |desk_event| %>
      <tr>
        <td><%= desk_event.event_id %></td>
        <td><%= desk_event.desk_id %></td>
        <td><%= link_to 'Show', desk_event %></td>
        <td><%= link_to 'Edit', edit_desk_event_path(desk_event) %></td>
        <td><%= link_to 'Destroy', desk_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Desk Event', new_desk_event_path %>
